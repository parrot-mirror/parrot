    scheme.test squaak.test \
    urm.test unlambda.test \
    WMLScript.test \
    Zcode.test


smoke:  all
	$(PERL) t/harness --html
	$(PERL) ../tools/util/smokeserv-client.pl languages_smoke.html

smoke-clean :
	$(RM_F) languages_smoke.html

