/*
 * Sample class used to verify read-only variant
 * generation. For testing only.
 */

pmclass ROTest dynpmc provides scalar extends Integer {
    void set_integer_native(INTVAL value) :read {
    }
    INTVAL get_integer() :write {
        return 42;
    }

    METHOD reader() {
        PCCRETURN(INTVAL 43);
    }

    METHOD writer(INTVAL ignored) :write {
        UNUSED(ignored);
        PCCRETURN(INTVAL 44);
    }
}


